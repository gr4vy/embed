{
  "name": "@gr4vy/embed",
  "version": "0.0.18",
  "description": "Quickly embed a credit card form in your web app and store the card details, authorize the card, and capture a transaction.",
  "main": "cjs/index.js",
  "module": "react/index.js",
  "author": "Gr4vy <code@gr4vy.com>",
  "license": "MIT",
  "repository": "github:gr4vy/embed",
  "homepage": "https://gr4vy.com",
  "scripts": {
    "build:package": "concurrently 'yarn build:cjs' 'yarn build:react' 'yarn build:react-umd'",
    "build:cjs": "webpack --config webpack/cjs.js",
    "build:umd": "webpack --config webpack/umd.js",
    "build:react": "babel src --ignore 'src/umd/*.js' --ignore 'src/dev.js' --out-dir react --copy-files --no-copy-ignored",
    "build:react-umd": "webpack --config webpack/react-umd.js",
    "clean": "rm -rf umd react cjs react-umd storybook-static *.tgz static coverage",
    "dev": "concurrently 'yarn start' 'yarn storybook' 'yarn test!'",
    "lint": "yarn lint:css; yarn lint:js",
    "lint:js": "yarn eslint src/**/*js tests/**/*js stories/**/*.js webpack/**/*.js .storybook/**/*.js *.js",
    "lint:css": "yarn stylelint src/**/*.scss",
    "release": "yarn version; git push --tags; git push",
    "start": "webpack serve --config webpack/dev-react.js",
    "start:umd": "webpack serve --config webpack/dev-umd.js",
    "storybook": "start-storybook -p 6007",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test!": "jest --watchAll --collect-coverage"
  },
  "files": [
    "cjs/**/*",
    "react/**/*",
    "react-umd/**/*",
    "docs/**/*"
  ],
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.0.5",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^6.0.5",
    "@storybook/addon-links": "^6.0.5",
    "@storybook/addon-storyshots": "^6.0.5",
    "@storybook/addons": "^6.0.5",
    "@storybook/react": "^6.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "concurrently": "^5.2.0",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.20.3",
    "form-napper": "^2.0.0",
    "html-webpack-plugin": "^5.0.0-alpha.10",
    "jest": "^26.3.0",
    "jest-css-modules": "^2.1.0",
    "nock": "^13.0.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "svg-url-loader": "^6.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "framebus": "^5.0.0",
    "uuid": "^8.3.1"
  }
}
