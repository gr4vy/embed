{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-typescript",
    "react-css-modules",
    "@babel/plugin-transform-runtime"
  ],
  "ignore": ["**/*.d.ts"]
}
